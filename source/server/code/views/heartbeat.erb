<script>
'use strict';
$(() => {

  cd.dashboardHeartbeat = () => {
    // This is a public cd.function so its callable
    // when auto-refresh/minute-columns checkboxes are clicked.
    if (cd.autoRefresh.isChecked()) {
      cd.refreshDashboard();
    }
  };

  const milliSecond = 1;
  const seconds = 1000 * milliSecond;
  setInterval(cd.dashboardHeartbeat, 10*seconds);

});
</script>
